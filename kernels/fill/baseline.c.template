#include "data.h"

#include <snrt.h>

#include <stdint.h>

#ifdef __cplusplus
extern "C" {
#endif

void fill(const DTYPE x, DTYPE* y) {
    for (uint32_t i = 0; i < M; ++i) {
        for (uint32_t j = 0; j < N; ++j) {
            // row-major accesses
            y[i * N + j] = x;
        }
    }
}

#ifdef __cplusplus
}
#endif
